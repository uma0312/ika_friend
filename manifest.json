{
	"name": "sample",
	"description": "イカナカマ監視",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts": [
		{
			"js": [
					"jquery-2.2.0.min.js",
					"content_script.js"
			],
			"matches": [
					"https://splatoon.nintendo.net/"
			],
			"run_at": "document_idle",
			"all_frames": true
		}
	],
	"background": {
		"scripts": ["background.js"],
		"presistent": false
	},
	"permissions": ["notifications"]
}